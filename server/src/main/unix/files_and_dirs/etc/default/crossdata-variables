SCRIPTNAME="/etc/init.d/$NAME"
USER="root"
PIDFILE="/var/run/sds/crossdata.pid"
LOGFILE="/var/log/sds/crossdata/server/crossdata.log"
LOCKFILE="/var/lock/subsys/$NAME.lock"
